# This is an example .flake8 config, used when developing *Black* itself.
# Keep in sync with setup.cfg which is used for source packages.

[flake8]
select = B,C,E,F,W,T4,B9
max-line-length = 120
max-complexity = 18
ignore = E265,
         F821,W503,E741,W504,E129,E731,
            #E501,E266,E128,E126,E722,E124,E251,E127,E125
            #F401,F841,E203,E221,C901,C408, E741,C407, C404,E122,E266,E262,
# C404 dict comprehension ignored due to https://github.com/pytorch/pytorch/issues/41448
exclude = build
per-file-ignores = __init__.py:F401,F403,E402
